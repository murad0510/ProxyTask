<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="BlueAndAqua">
        <GradientStop Color="Aqua" Offset="0.4"></GradientStop>
        <GradientStop Color="Blue" Offset="1"></GradientStop>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="White" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="DeepSkyBlue" Color="DeepSkyBlue"></SolidColorBrush>

</ResourceDictionary>
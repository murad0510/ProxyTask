<Application x:Class="ProxyTask.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProxyTask"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictionary/TextBlockStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictionary/ButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictionary/TextBoxStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Dictionary/ListBoxStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
